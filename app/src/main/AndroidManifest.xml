<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.s1.login_">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signUpScreen"
            android:label="signUpScreen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".signInScreen"
            android:label="@string/title_activity_sign_in_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".resetPassword"
            android:label="@string/title_activity_reset_password"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".choosingSubjectSCreen"
            android:label="@string/title_activity_choosing_subject_screen"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>